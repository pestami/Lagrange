\begin_body

\begin_layout Part
Lagrange Equations
\end_layout

\begin_layout Paragraph
Example 3.3 Motion of mass on plane
\end_layout

\begin_layout Paragraph
\begin_inset Formula $\frac{\partial}{\partial t}\left(\frac{\partial T}{\partial\stackrel{.}{\theta}}\right)-\frac{\partial T}{\partial q_{r}}=F_{q_{r}}$
\end_inset


\end_layout

\begin_layout Paragraph
Kinetic Energy in polar cordintes:
\end_layout

\begin_layout Paragraph
\begin_inset Formula $T=\frac{m}{2}\left(\stackrel{.}{r}^{2}+\left(r\stackrel{.}{\theta}\right)^{2}\right)$
\end_inset

 
\end_layout

\begin_layout Paragraph
Transformations Equations:
\end_layout

\begin_layout Paragraph
\begin_inset Formula $x=r\cdot cos(\theta)$
\end_inset


\end_layout

\begin_layout Paragraph
\begin_inset Formula $y=r\cdot sin(\theta)$
\end_inset


\end_layout

\begin_layout Paragraph
Lagrange for this case :
\end_layout

\begin_layout Paragraph
\begin_inset Formula $\frac{\partial}{\partial t}\left(\frac{\partial T}{\partial\stackrel{.}{r}}\right)-\frac{\partial T}{\partial r}=F_{r}$
\end_inset

 and 
\begin_inset Formula $\frac{\partial}{\partial t}\left(\frac{\partial T}{\partial\stackrel{.}{\theta}}\right)-\frac{\partial T}{\partial\theta}=F_{\theta}$
\end_inset

 
\end_layout

\begin_layout Paragraph

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $F_{\theta}=0$
\end_inset

 no tangental force.
 
\begin_inset Formula $F_{r}=k\left(R_{0}-r\right)$
\end_inset

 Hooks law ,tangental force.
\end_layout

\begin_layout Paragraph
Now to evaluate terms: 
\begin_inset Formula $\frac{\partial T}{\partial\theta}$
\end_inset

 & 
\begin_inset Formula $\left(\frac{\partial T}{\partial\stackrel{.}{\theta}}\right)$
\end_inset

& 
\begin_inset Formula $\frac{\partial T}{\partial\stackrel{.}{r}}$
\end_inset

& 
\begin_inset Formula $\frac{\partial}{\partial t}\left(\frac{\partial T}{\partial\stackrel{.}{r}}\right)$
\end_inset

 :
\end_layout

\begin_layout Paragraph
\begin_inset Formula $T=\frac{m}{2}\left(\stackrel{.}{r}^{2}+\left(r\stackrel{.}{\theta}\right)^{2}\right)$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\partial T}{\partial\theta}=\frac{m}{2}\left(2\stackrel{.}{r}\frac{\delta r}{\partial\theta}+2r\stackrel{.}{\theta}\frac{\delta}{\partial\theta}\left(\stackrel{.}{r}\stackrel{.}{\theta}+r\stackrel{..}{\theta}\right)\right)$
\end_inset

 
\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{\partial T}{\partial\theta}=m\left(\stackrel{.}{r}\frac{\delta r}{\partial\theta}+2r\stackrel{.}{\theta}\left(u'v+uv'+p'q+pq'\right)\right)$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{\partial T}{\partial\theta}=m\left(\stackrel{.}{r}\frac{\delta r}{\partial\theta}+2r\stackrel{.}{\theta}\left(\frac{\delta\stackrel{.}{r}}{\partial\theta}\stackrel{.}{\theta}+\stackrel{.}{r}\frac{\delta\stackrel{.}{\theta}}{\partial\theta}+\frac{\delta r}{\partial\theta}\stackrel{..}{\theta}+r\frac{\delta\stackrel{..}{\theta}}{\partial\theta}\right)\right)$
\end_inset


\end_layout

\begin_layout Paragraph*
Some terms evaluate to zero:
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\delta\stackrel{..}{\theta}}{\partial\theta}=\frac{\delta}{\partial t}\frac{\delta\stackrel{.}{\theta}}{\partial\theta}=\frac{\delta}{\partial t}\frac{\delta}{\partial t}\frac{\delta\stackrel{}{\theta}}{\partial\theta}=\frac{\delta}{\partial t}\frac{\delta}{\partial t}1=0$
\end_inset

 similar logic 
\begin_inset Formula $\frac{\delta\stackrel{.}{\theta}}{\partial\theta}=0$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{\partial T}{\partial\theta}=m\left(\stackrel{.}{r}\frac{\delta r}{\partial\theta}+2r\stackrel{.}{\theta}\left(\frac{\delta\stackrel{.}{r}}{\partial\theta}\stackrel{.}{\theta}+\frac{\delta r}{\partial\theta}\stackrel{..}{\theta}\right)\right)$
\end_inset


\end_layout

